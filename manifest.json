{
	"manifest_version": 2,
	"name": "Floribundus",
	"version": "0.0.2",
	"description": "Sort tabs",
	"icons": {
		"48": "icons/icon_gray.png",
		"96": "icons/icon_gray.png"
	},
	"background": {
		"scripts": ["floribundus_background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icons/icon_gray.png",
		"default_title": "Sort tabs"
	},
	"permissions": ["tabs"]
}
